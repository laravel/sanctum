{
    "name": "laravel/airlock",
    "description": "Laravel Airlock provides a featherweight authentication system for SPAs and simple APIs.",
    "keywords": ["laravel", "airlock", "auth"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel/airlock/issues",
        "source": "https://github.com/laravel/airlock"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "illuminate/contracts": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Airlock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Airlock\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Airlock\\AirlockServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
